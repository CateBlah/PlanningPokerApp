<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <listener>
        <listener-class>MyListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Controllers.LoginController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegisterController</servlet-name>
        <servlet-class>Controllers.RegisterController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterController</servlet-name>
        <url-pattern>/RegisterController</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserHomeController</servlet-name>
        <servlet-class>Controllers.UserHomeController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserHomeController</servlet-name>
        <url-pattern>/UserHomeController</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProjectHomeController</servlet-name>
        <servlet-class>Controllers.ProjectHomeController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProjectHomeController</servlet-name>
        <url-pattern>/ProjectHomeController</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserStoryController</servlet-name>
        <servlet-class>Controllers.UserStoryController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserStoryController</servlet-name>
        <url-pattern>/UserStoryController</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AccountSettingsController</servlet-name>
        <servlet-class>Controllers.AccountSettingsController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccountSettingsController</servlet-name>
        <url-pattern>/AccountSettingsController</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProjectAJAXServlet</servlet-name>
        <servlet-class>Controllers.ProjectAJAXServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProjectAJAXServlet</servlet-name>
        <url-pattern>/ProjectAJAXServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProgressJsonServlet</servlet-name>
        <servlet-class>Controllers.ProgressJsonServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProgressJsonServlet</servlet-name>
        <url-pattern>/ProgressJsonServlet</url-pattern>
    </servlet-mapping>
</web-app>